<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>pacienteController</servlet-name>
        <servlet-class>com.clinica.controladores.PacienteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CitaController</servlet-name>
        <servlet-class>com.clinica.controladores.CitaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultaController</servlet-name>
        <servlet-class>com.clinica.controladores.ConsultaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiciosController</servlet-name>
        <servlet-class>com.clinica.controladores.ServiciosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosController</servlet-name>
        <servlet-class>com.clinica.controladores.UsuariosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.clinica.controladores.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pacienteController</servlet-name>
        <url-pattern>/PacienteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CitaController</servlet-name>
        <url-pattern>/CitaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultaController</servlet-name>
        <url-pattern>/ConsultaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiciosController</servlet-name>
        <url-pattern>/ServiciosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosController</servlet-name>
        <url-pattern>/UsuariosController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
